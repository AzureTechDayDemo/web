@page
@model AboutModel
<style type="text/css">
    body {
        background-color: green;
    }
</style>
<h2>@ViewData["Title"]</h2>
<h3>@Model.Message</h3>
<h2>Registry jengademos.azurecr.io IP: @Model.RegistryIp</h2>

<b>HostName:</b> @Model.HostName<br />
<b>OS Architecture:</b> @Model.OsArchitecture<br />
<b>OS Description:</b> @Model.OsDescription<br />
<b>Process Architecture:</b> @Model.ProcessArchitecture<br />
<b>Framework Description:</b> @Model.FrameworkDescription<br />
<b>ASP.NET Core Package Version</b> @Model.AspNetCorePackageVersion<br />
<b>ASP.NET Hosting Environment:</b> @Model.AspNetCoreEnvironment<br />

<h3>Environment Variables:</h3>
@Html.Raw(@Model.EnvironmentVariables)
